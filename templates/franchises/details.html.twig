{% extends 'base.html.twig' %}

{% block title %}{{ franchise.name }}- Crossfit_life{% endblock %}

{% block body %}
    <div class="text">{{ franchise.name }}</div>
    <main class="container">
        <section class="row">
            <div class="col-12">
                {% for structure in structures %}
                    <div class="col-3">
                        <article class="card">
                            <div class="card-body">
                                <h2 class="card-title">{{ structure.address }}</h2>
                                {% for  in  %}
                                    <p class="card-text"></p>
                                {% endfor %}
                            </div>
                        </article>
                    </div>
                {% endfor %}
            </div>
        </section>
    </main>
{% endblock %}